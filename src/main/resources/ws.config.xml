<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ws-configuration
    	PUBLIC "-//WS/WS Configuration DTD//EN"
    	"http://lsr.cn/ws-config.dtd">
<wsconfig> 
	<interceptors-define>
		<interceptor id="exceptionHandle" type="com.lsr.ws.interceptor.impl.ExceptionHandleInterceptor"/>
		<interceptor id="sessionValidate" type="com.lsr.ws.interceptor.impl.SessionValidateInterceptor"/>
		<!--
		<interceptor id="hibernateSessionOpen" type="com.lsr.ws.interceptor.impl.HibernateSessionOpenInterceptor"/>
		<interceptor id="security" type="com.lsr.ws.interceptor.impl.SecurityInterceptor"/>
		<interceptor id="compress" type="com.lsr.ws.interceptor.impl.CompressInterceptor"/>
		-->
		<interceptor id="ztThread" type="com.lsr.ws.interceptor.impl.ThreadInterceptor"/>
	</interceptors-define>

	<global-define>
		<interceptors-ref references="exceptionHandle,sessionValidate,ztThread"/>
	</global-define>

	<config-include>
		<resource path="wsconfig/index.ws.config.xml"/>
	</config-include>
</wsconfig>
