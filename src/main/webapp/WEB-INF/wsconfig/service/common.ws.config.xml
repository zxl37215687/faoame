<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ws-configuration
    	PUBLIC "-//WS/WS Configuration DTD//EN"
    	"http://lsr.cn/ws-config.dtd">
<wsconfig>
	<wsvos-define package="com.lsr.common.vo">
		<wsvo id="LabelValue" type="LabelValue">
			<wsvoproperty id="LABEL" property="label" />
			<wsvoproperty id="VALUE" property="value" />
		</wsvo>
	</wsvos-define>
	
	
	<wsactions-define >
		<wsaction id="testWSAction"
				type="com.lsr.common.remind.wsaction.testWSAction"
				scope="application">
			<intercept-ignore-set>
 				<intercept-ignore  interceptorId="sessionValidate"  actionMethods="toList"></intercept-ignore>
 			</intercept-ignore-set>
		</wsaction>
	</wsactions-define>
		
</wsconfig>
