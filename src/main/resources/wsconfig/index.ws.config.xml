<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ws-configuration
    	PUBLIC "-//WS/WS Configuration DTD//EN"
    	"http://lsr.cn/ws-config.dtd">
<wsconfig>
	<wsvos-define package="com.lsr.index.vo"> 
		<wsvo id="IndexBookVO" type="IndexBookVO">
			<wsvoproperty id="FZTID" property="ztid" />
			<wsvoproperty id="FID" property="id" />
			<wsvoproperty id="FINDEX_CODE" property="indexCode" />
		</wsvo>
		
		<wsvo id="IndexBookConvVO" type="IndexBookConvVO">
			<wsvoproperty id="FZTID" property="ztid" />
			<wsvoproperty id="FYEAR" property="year" />
			<wsvoproperty id="FID" property="id" />
			<wsvoproperty id="FINDEX_BOOK_ID" property="indexBookId" />
		</wsvo>
		
	</wsvos-define>	
	
	<wsvos-define package="com.lsr.budget2.outlay.vo">
		<wsvo id="IndexChangeVO" type="IndexChangeVO">
			<wsvoproperty id="FPRJ_CODE" property="prjCode" />
			<wsvoproperty id="FATTRIBUTE_ORG_NAME" property="attributeOrgName" />
			<wsvoproperty id="FROOT_TYPE_NAME" property="rootTypeName" />
		</wsvo>
	</wsvos-define>

	<wsactions-define package="com.lsr.index.wsaction">
		<wsaction id="IndexBillWSAction" type="IndexBillWSAction" scope="application"></wsaction>
		<wsaction id="IndexBookWSAction" type="IndexBookWSAction" scope="application"></wsaction>

	</wsactions-define>
	<wsactions-define package="com.lsr.index.util">
		<wsaction id="CustomFundsrc" type="CustomFundsrc" scope="application"></wsaction>
	</wsactions-define>
	
</wsconfig>
